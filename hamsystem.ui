QPushButton:hover {
    background-color: rgba(30, 34, 43, 0.05);  /* 悬浮时背景轻微变暗 */
    border: 1px solid rgba(30, 34, 43, 0.2);   /* 悬浮时边框加深 */
}

QPushButton:pressed {
    background-color: rgba(30, 34, 43, 0.1);   /* 按下时背景更暗 */
    border: 1px solid rgba(30, 34, 43, 0.3);   /* 按下时边框更深 */
    padding: 2px 15px 0px 15px;                /* 轻微的按下效果 */
}

QPushButton:disabled {
    color: rgba(30, 34, 43, 0.3);              /* 禁用时文字变淡 */
    background-color: rgba(228,228,228,0.3);   /* 禁用时背景变灰 */
    border: 1px solid rgba(228,228,228,0.5);   /* 禁用时边框变淡 */
}

/* 如果按钮包含图标 */
QPushButton:icon {
    padding: 1px 15px 1px 10px;  /* 左侧padding适当调整以适应图标 */
}
/* 确认和取消按钮样式 */
QDialogButtonBox {
    spacing: 10px;
}

QDialogButtonBox QPushButton {
    min-width: 90px;
    min-height: 35px;
    border-radius: 8px;
    font-family: &quot;Microsoft YaHei&quot;;
    font-size: 14px;
    padding: 5px 20px;
}
/* OK按钮样式 */
